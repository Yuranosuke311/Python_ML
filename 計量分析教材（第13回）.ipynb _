\n","\n","# Xはpandas DataFrameなので、X.valuesでnumpyにします\n","X = torch.from_numpy(X).float()\n","y = torch.from_numpy(y).long()\n","\n","# 80% train 20% test\n","X_train, X_test, y_train, y_test = train_test_split(X,y, test_size=0.2, random_state=0)\n","\n","# Dataloader\n","\n","from torch.utils.data import TensorDataset, DataLoader\n","train_dataset = TensorDataset(X_train, y_train)\n","train_dataloader = DataLoader(train_dataset, batch_size=32, shuffle=True, drop_last=True)\n","\n","test_dataset = TensorDataset(X_test, y_test)\n","test_dataloader = DataLoader(test_dataset, batch_size=32, shuffle=False, drop_last=False)"],"metadata":{"id":"XM61wObWg8XQ"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["# 精度を高める方法\n","\n","1. チャンネルの次元を増やす\n","\n","2. 隠れ畳み込み層の数を増やす\n","\n","3. バッチ正規化など試してみる"],"metadata":{"id":"YL_HTx5Q_ncj"}},{"cell_type":"code","source":["# CNNニューラルネットワークの設計\n","\n","import torch\n","import torch.nn as nn\n","import torch.nn.functional as F\n","\n","class My_CNN_new(nn.Module):\n","    def __init__(self, size, n_output):\n","        # size: 画像の高さ（長さ） GAPに使う\n","\n","        super(My_CNN_new, self).__init__()\n","        self.conv1 = nn.Conv2d(in_channels=3,  out_channels=64, kernel_size=3, padding=1, stride=1)\n","        self.conv2 = nn.Conv2d(in_channels=64, out_channels=128, kernel_size=3, padding=1, stride=1)\n","        self.conv3 = nn.Conv2d(in_channels=128, out_channels=256, kernel_size=3, padding=1, stride=1)\n","        self.conv4 = nn.Conv2d(in_channels=256, out_channels=256, kernel_size=3, padding=1, stride=1)\n","        self.pool = nn.MaxPool2d(kernel_size=2)\n","\n","        # 画像サイズと同じサイズのカーネルをかけることで、スカラーに変換できます。\n","        # カーネルサイズが size / 4の理由：\n","        # forward()で、poolを二回かけていますので、\n","        self.global_pool = nn.AvgPool2d(kernel_size=int(size / 4))\n","        self.fc= nn.Linear(256, n_output)\n","\n","    def forward(self,x):\n","        x = F.relu(self.conv1(x))\n","        x = self.pool(x)\n","        x = F.relu(self.conv2(x))\n","        x = self.pool(x)\n","        x = F.relu(self.conv3(x))\n","        x = F.relu(self.conv4(x))\n","\n","        # GAP\n","        x = self.global_pool(x)\n","        # (data, 64, 1) -> (data, 64)\n","        x = x.reshape(-1, 256) # (1 * channel数にreshape)\n","        x = self.fc(x)\n","        return x\n"],"metadata":{"id":"c8Yu0c1N0tCQ"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# ここから\n","from tqdm._tqdm_notebook import tqdm_notebook as tqdm\n","from torch import optim\n","from sklearn.metrics import accuracy_score\n","import numpy as np\n","\n","Epochs = 50 # エポック数\n","lr = 1e-3\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","print(device)\n","model = My_CNN_new(32, 10).to(device) # <- GPUに\n","criterion = nn.CrossEntropyLoss()\n","optimizer = optim.Adam(model.parameters(), lr=lr)\n","\n","losses = torch.zeros(Epochs)\n","accs = torch.zeros(Epochs)\n","torch.manual_seed(0)\n","for epoch in tqdm(range(Epochs)):\n","  model.train()\n","\n","  total_loss = 0\n","  for x_batch, y_batch in train_dataloader:\n","    x_batch = x_batch.to(device)\n","    y_batch = y_batch.to(device)\n","    fcst = model(x_batch) # <- GPUに\n","    loss = criterion(fcst, y_batch) # <- GPUに\n","    loss.backward()\n","    optimizer.step()\n","    optimizer.zero_grad()\n","\n","    total_loss += loss.item()\n","\n","  # ログ\n","  losses[epoch] = total_loss\n","\n","  print(epoch, '損失:',loss.item())\n","\n","  model.eval()\n","\n","  # テスト評価\n","  #x_batch = x_batch.to(device)\n","  fcst = model(X_test.to(device)) # <- GPUに\n","  score = accuracy_score(y_test, fcst.to('cpu').argmax(axis = 1)) # <- 精度比較の際にはcpuで\n","  accs[epoch] = score\n","  print(epoch, f'テスト正解率:{score:.2f}')"],"metadata":{"id":"89-DeKtTVS-i","colab":{"base_uri":"https://localhost:8080/","height":1000,"referenced_widgets":["ce944b7f6e664259830d15b9f0b259e1","ea5f8e1b138c4044ab2e3b4f7f25ad80","23feb83da17b4790a7adbce0b61801bd","9db25d6cf69f42c697