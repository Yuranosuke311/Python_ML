"\n","test_dataset = TensorDataset(X_test, y_test)\n","test_dataloader = DataLoader(test_dataset, batch_size=32, shuffle=False, drop_last=False)"],"metadata":{"id":"TyQ5EZnXl_bx"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["比較用のモデル"],"metadata":{"id":"oAUKFydYjSX6"}},{"cell_type":"code","source":["from torch import nn\n","import numpy as np\n","import torch.optim as optim\n","import torch.nn.functional as F\n","\n","class My_net(nn.Module):#継承\n","  def __init__(self, n_input, n_output):  #n_input : 入力層の次元, n_output: 出力層の次元\n","    super().__init__() #基底クラス\n","    self.input_layer = nn.Linear(n_input, 64)# 入力層 -> 隠れ層1\n","    self.hidden_layer =  nn.Linear(64, 32)# 隠れ層1 -> 隠れ層2\n","    self.output_layer = nn.Linear(32, n_output)# 隠れ層2 -> 出力層\n","\n","    # 変更点\n","    # バッチ正規化\n","    self.bn0 = nn.BatchNorm1d(64) # input -> hiddenに挟む\n","    self.bn1 = nn.BatchNorm1d(32) # hidden -> inputに挟む\n","\n","\n","  def forward(self, x):# 2. ニューラルネットワークの予測関数を定義します。\n","\n","    x = F.relu(self.input_layer(x))\n","    x = self.bn0(x)\n","    x = F.relu(self.hidden_layer(x))\n","    x = self.bn1(x)\n","    x = self.output_layer(x)\n","    return x\n"],"metadata":{"id":"14aLkak6lPjS"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from tqdm._tqdm_notebook import tqdm_notebook as tqdm\n","from torch import optim\n","from sklearn.metrics import accuracy_score\n","import numpy as np\n","import torch\n","\n","Epochs = 100 # エポック数\n","lr = 1e-3\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","print(device)\n","model = My_net(32*32 * 3, 10).to(device)\n","criterion = nn.CrossEntropyLoss()\n","optimizer = optim.Adam(model.parameters(), lr=lr)\n","\n","losses = torch.zeros(Epochs)\n","accs = torch.zeros(Epochs)\n","torch.manual_seed(0)\n","for epoch in tqdm(range(Epochs)):\n","  model.train()\n","  total_loss = 0\n","  for x_batch, y_batch in train_dataloader:\n","    x_batch = x_batch.to(device)\n","    y_batch = y_batch.to(device)\n","    fcst = model(x_batch)\n","    loss = criterion(fcst, y_batch)\n","    loss.backward()\n","    optimizer.step()\n","    optimizer.zero_grad()\n","    total_loss += loss.item()\n","  losses[epoch] = total_loss\n","  print(epoch,total_loss)\n","\n","  model.eval()\n","  size = 0\n","  score = 0\n","  for x_batch, y_batch in test_dataloader:\n","    x_batch = x_batch.to(device)\n","    fcst = model(x_batch)\n","    size += y_batch.shape[0]\n","    score += y_batch.shape[0] * accuracy_score(y_batch, fcst.to('cpu').argmax(axis = 1))\n","  accs[epoch] = score/size\n","  print(epoch, score/size)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000,"referenced_widgets":["3dd7bf3d08b14cc898048d4184628bc7","12ec1bdde2374fb7b84d9b444a6408f1","4e44e2999e654afb9347139cfb98e982","2d76dd7d1d224fb8b36b1870dfedb49b","cde3ceec4b16483c85a3661e3ba7eca4","c39b7a570aad4cd0b9cd010da04aac5f","0655ac27ea97408a90cde4ef024eb2d7","10036f493dcc455e99b5f36760c3806b","bf6e02d94f0c458c988fcd269c1eacc4","c275d52bca964f39aed18f77ad64f22d","5c5b6cd32fe34f628c285483e7753d34"]},"id":"A7Rqq-eZldNC","executionInfo":{"status":"ok","timestamp":1721351018442,"user_tz":-540,"elapsed":38058,"user":{"displayName":"ゴライアスガエル","userId":"08595973168675802700"}},"outputId":"3be1d3d9-cf57-448e-9765-cb5a54a53f06"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["cuda\n"]},{"output_type":"display_data","data":{"text/plain":["  0%|          | 0/100 [00:00<?, ?it/s]"],"application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"3dd7bf3d08b14cc898048d4184628bc7"}},"metadata":{}},{"output_type":"stream","name":"stdout","text":["0 253.64205527305603\n","0 0.392\n","1 200.1013001203537\n","1 0.347\n","2 171.92377197742462\n","2 0.459\n","3 152.19477885961533\n","3 0.485\n","4 134.2656610608101\n","4 0.435\n","5 128.37549656629562\n","5 0.575\n","6 116.81890952587128\n","6 0.505\n","7 110.59472614526749\n","7 0.525\n